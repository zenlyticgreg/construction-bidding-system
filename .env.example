# Caltrans Bidding System Environment Variables
# Copy this file to .env and modify values as needed

# Debug mode (True/False)
DEBUG=True

# Maximum file size for uploads
MAX_FILE_SIZE=100MB

# Default markup percentage (as decimal)
DEFAULT_MARKUP=0.20

# Delivery percentage (as decimal)
DELIVERY_PERCENTAGE=0.03

# Minimum delivery fee
MIN_DELIVERY_FEE=150.00

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
