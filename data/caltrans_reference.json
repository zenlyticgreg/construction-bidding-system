  "quantity_patterns": {
    "measurement_units": {
      "SQFT": {
        "regex_patterns": [
          "(\\d+(?:\\.\\d+)?)\\s*(?:SQ\\s*FT|SQFT|SF|SQUARE\\s*FEET?)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:SQ\\s*YARDS?|SQYD|SY)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:SQ\\s*METERS?|SQM)"
        ],
        "conversions": {
          "sqft_to_sqyd": 0.111,
          "sqft_to_sqm": 0.093,
          "sqyd_to_sqft": 9.0,
          "sqm_to_sqft": 10.764
        }
      },
      "LF": {
        "regex_patterns": [
          "(\\d+(?:\\.\\d+)?)\\s*(?:LF|LINEAR\\s*FEET?|LINEAR\\s*FT)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:LY|LINEAR\\s*YARDS?)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:LM|LINEAR\\s*METERS?)"
        ],
        "conversions": {
          "lf_to_ly": 0.333,
          "lf_to_lm": 0.305,
          "ly_to_lf": 3.0,
          "lm_to_lf": 3.281
        }
      },
      "CY": {
        "regex_patterns": [
          "(\\d+(?:\\.\\d+)?)\\s*(?:CY|CUBIC\\s*YARDS?|CU\\s*YD)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:CF|CUBIC\\s*FEET?|CU\\s*FT)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:CM|CUBIC\\s*METERS?|CU\\s*M)"
        ],
        "conversions": {
          "cy_to_cf": 27.0,
          "cy_to_cm": 0.765,
          "cf_to_cy": 0.037,
          "cm_to_cy": 1.308
        }
      },
      "EA": {
        "regex_patterns": [
          "(\\d+(?:\\.\\d+)?)\\s*(?:EA|EACH|EACHES?)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:PCS?|PIECES?)",
          "(\\d+(?:\\.\\d+)?)\\s*(?:UNITS?|UN)"
        ],
        "conversions": {
          "ea_to_pcs": 1.0,
          "pcs_to_ea": 1.0
        }
      }
    },
    "quantity_extraction": {
      "patterns": {
        "quantity_with_unit": "(\\d+(?:,\\d{3})*(?:\\.\\d+)?)\\s*([A-Z]{2,4})",
        "quantity_in_text": "(?:QUANTITY|QTY|AMOUNT)[:\\s]*(\\d+(?:,\\d{3})*(?:\\.\\d+)?)",
        "range_quantity": "(\\d+(?:\\.\\d+)?)\\s*-\\s*(\\d+(?:\\.\\d+)?)\\s*([A-Z]{2,4})",
        "approximate_quantity": "(?:APPROX|EST|~)\\s*(\\d+(?:\\.\\d+)?)\\s*([A-Z]{2,4})"
      },
      "default_units": {
        "concrete": "CY",
        "formwork": "SQFT",
        "reinforcement": "LB",
        "structural": "EA"
      }
    }
  },
